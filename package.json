{
  "name": "apologeticcookie",
  "version": "1.0.0",
  "description": "Next Generation Photo Exploration App",
  "main": "index.js",
  "scripts": {
    "postinstall": "npm run build:prod",
    "start": "node ./server/app.js",
    "test": "npm run test:client && npm run test:server",
    "test:client": "karma start --single-run",
    "test:client:watch": "karma start",
    "test:server": "mocha ./test/server/.setup.js test/server/*.spec.js test/server/*/*.spec.js",
    "lint": "eslint ./",
    "build": "webpack",
    "build:prod": "webpack --prod",
    "build:watch": "webpack --watch",
    "serve": "webpack-dev-server --content-base client --progress --watch --history-api-fallback --inline --hot --host 0.0.0.0 --port 9999"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/apologeticcookie/phodome.git"
  },
  "keywords": [
    "react",
    "material-ui",
    "aframe",
    "postgresSQL",
    "node",
    "express"
  ],
  "author": "John Oksasoglu, Jon Eleiott, Riyaz Ahmed, Tony K. Tan",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/apologeticcookie/phodome/issues"
  },
  "homepage": "https://github.com/apologeticcookie/phodome#readme",
  "dependencies": {
    "aframe": "^0.3.2",
    "aframe-animation-component": "^3.0.4",
    "aframe-bmfont-text-component": "^0.1.4",
    "aframe-react": "^3.2.3",
    "aframe-text-component": "^0.3.0",
    "axios": "^0.15.2",
    "babel-core": "^6.3.15",
    "babel-loader": "^6.2.0",
    "babel-polyfill": "^6.3.14",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "babel-preset-stage-0": "^6.3.13",
    "body-parser": "^1.15.2",
    "chai-as-promised": "^6.0.0",
    "compression": "^1.6.2",
    "css-loader": "^0.23.0",
    "express": "^4.14.0",
    "formidable": "*",
    "gm": "*",
    "material-ui": "^0.16.1",
    "morgan": "^1.7.0",
    "node-static": "*",
    "pg": "^6.1.0",
    "pg-hstore": "^2.3.2",
    "react": "^15.3.2",
    "react-addons-css-transition-group": "^15.3.2",
    "react-dom": "^15.3.2",
    "react-fine-uploader": "^0.1.1",
    "react-tap-event-plugin": "^1.0.0",
