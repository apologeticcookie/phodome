    "babel-runtime": "^6.3.13",
    "chai": "^3.5.0",
    "chai-enzyme": "^0.5.2",
    "enzyme": "^2.5.1",
    "eslint": "^3.8.1",
    "eslint-config-hackreactor": "git://github.com/reactorcore/eslint-config-hackreactor",
    "eslint-plugin-react": "^6.4.1",
    "jasmine-core": "^2.5.2",
    "jsdom": "^9.8.0",
    "json-loader": "^0.5.4",
    "karma": "^1.3.0",
    "karma-babel-preprocessor": "^6.0.1",
    "karma-chrome-launcher": "^2.0.0",
    "karma-jasmine": "^1.0.2",
    "karma-mocha": "^1.2.0",
